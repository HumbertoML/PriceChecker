{
    "name": "NASA picture of the day viewer",
    "version": "0.0.0.1",
    "manifest_version": 2,
    "description": "_Extensi√≥n de Chromium para seguir productos en de tiendas online y visualizar sus precios a lo largo del tiempo_",
    "icons": {
        "16": "icons/icon_16.png",
        "32": "icons/icon_32.png",
        "48": "icons/icon_48.png",
        "128": "icons/icon_128.png",
        "400": "icons/icon_400.png"
    },
    "browser_action": {
        "default_popup": "popup/popup.html"
      },
      "content_scripts": [
        {
          "matches": [
            "<all_urls>"
          ],
          "js": ["lib/jquery.min.js",
            "scripts/content.js"
          ]
        }
      ],
      "web_accessible_resources": [
        "images/*.jpeg"
      ],
      "permissions":[
        "notifications",
        "storage",
        "tabs"
      ]
}